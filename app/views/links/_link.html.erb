<div id="<%= dom_id link %>">
  <p>
    <strong>User:</strong>
    <%= link.user_id %>
  </p>

  <p>
    <strong>Url:</strong>
    <%= link.url %>
  </p>

  <p>
    <strong>Url short:</strong>
    <%= link.url_short.chomp('==') %>
  </p>

  <p>
    <strong>Visits:</strong>
    <%= link.visits %>
  </p>

  <p>
    <strong>Category:</strong>
    <%= link.category %>
  </p>

</div>
